<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>

<VBox alignment="CENTER" prefHeight="400" prefWidth="400" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.emsi.gui.UpdateItemController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Matricule:" />
    <TextField fx:id="matriculeTextField" />
    <Label text="Marque:" />
    <TextField fx:id="marqueTextField" />
    <Label text="Couleur" />
    <TextField fx:id="couleurTextField" />
    <Label text="Prix" />
    <TextField fx:id="prixTextField" />
    <Label text="Kilometrage" />
    <TextField fx:id="kilometrageTextField" />
    <Label text="Vitesse" />
    <TextField fx:id="vitesseTextField" />

    <HBox alignment="CENTER" spacing="20.0">
    <Button fx:id="updateButton" text="Update" />
    <Button onAction="#cancel" text="Cancel" /></HBox>

</VBox>
